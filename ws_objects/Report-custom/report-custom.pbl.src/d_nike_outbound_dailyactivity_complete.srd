table(column=(type=char(16) updatewhereclause=yes name=do_no dbname="DO_No" )
 column=(type=datetime updatewhereclause=yes name=ord_date dbname="Ord_Date" )
 column=(type=datetime updatewhereclause=yes name=complete_date dbname="Complete_Date" )
 column=(type=char(1) updatewhereclause=yes name=inventory_type dbname="Inventory_Type" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=request_qty dbname="request_qty" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=ship_qty dbname="Ship_Qty" )
 column=(type=long updatewhereclause=yes identity=yes name=line dbname="Line" )
 retrieve="  SELECT Delivery_Master.DO_No,   
         Delivery_Master.Ord_Date,   
         Delivery_Master.Complete_Date,   
         Delivery_Master.Inventory_Type,   
         Sum(Delivery_Detail.Req_Qty) As request_qty,   
          IsNull((Select sum(Quantity)  From Delivery_Picking Where Delivery_Picking.Do_No =  Delivery_Master.Do_No  ),0) As Ship_Qty,   
         Count(Delivery_Detail.Do_No) As Line  
    FROM Delivery_Master, Delivery_Detail  
   WHERE ( Delivery_Master.Project_Id = :project_id ) AND
			( Delivery_Master.Do_No =  Delivery_Detail.Do_No  ) AND
			( Delivery_Master.WH_Code = :wh_code ) AND 
			( Delivery_Master.Ord_status = 'C' ) AND
         ( Delivery_Master.Complete_Date >= :s_date ) AND  
         ( Delivery_Master.Complete_Date < :e_date ) AND
         ( Delivery_Master.Inventory_Type <> 'P' OR  
         Delivery_Master.Inventory_Type = null )
Group By 
Delivery_Master.DO_No,   
         Delivery_Master.Ord_Date,   
         Delivery_Master.Complete_Date,   
         Delivery_Master.Inventory_Type
  
" arguments=(("project_id", string),("wh_code", string),("s_date", date),("e_date", date)) )
