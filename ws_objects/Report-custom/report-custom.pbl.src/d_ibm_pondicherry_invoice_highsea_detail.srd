 retrieve="SELECT  delivery_picking.sku ,
           Sum(delivery_picking.quantity) as quantity ,
           delivery_picking.po_no ,
           delivery_picking.lot_no ,
           delivery_picking.expiration_date ,
           item_master.description ,
           delivery_master.ship_via ,
           carrier_master.carrier_name ,
           delivery_master.awb_bol_no ,
           delivery_master.weight ,
           delivery_master.wh_code ,
           warehouse.address_1  
        FROM delivery_master LEFT OUTER JOIN carrier_master ON delivery_master.project_id = carrier_master.project_id AND delivery_master.carrier = carrier_master.carrier_code ,
           delivery_picking ,
           item_master ,
           warehouse    
        WHERE ( delivery_picking.supp_code = item_master.supp_code ) and
          ( delivery_picking.sku = item_master.sku ) and 
				(delivery_master.Project_id = Item_master.Project_id) and 
          ( delivery_master.do_no = delivery_picking.do_no ) and
          ( warehouse.wh_code = delivery_master.wh_code ) and
          ( ( delivery_master.do_no = :asdono )  )  

Group By delivery_picking.sku, 
           delivery_picking.po_no ,
           delivery_picking.lot_no ,
           delivery_picking.expiration_date ,
           item_master.description ,
           delivery_master.ship_via ,
           carrier_master.carrier_name ,
           delivery_master.awb_bol_no ,
           delivery_master.weight ,
           delivery_master.wh_code ,
           warehouse.address_1  " arguments=(("asdono", string),("asUser", string),("asAmountText", string)) )
