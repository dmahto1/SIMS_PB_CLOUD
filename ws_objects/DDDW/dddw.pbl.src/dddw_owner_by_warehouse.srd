table(column=(type=decimal(0) updatewhereclause=yes name=owner_owner_id dbname="Owner.Owner_Id" )
 column=(type=char(20) updatewhereclause=yes name=owner_owner_cd dbname="Owner.Owner_Cd" )
 column=(type=long updatewhereclause=yes name=protected dbname="Protected" )
 retrieve="   SELECT dbo.Owner.Owner_Id,   
         dbo.Owner.Owner_Cd,
			0 as Protected  
    FROM dbo.Owner,   
         dbo.Customer  
   WHERE ( dbo.Owner.Project_Id = dbo.Customer.Project_Id ) and  
         ( dbo.Owner.Owner_Cd = dbo.Customer.Cust_Code ) and  
         ( ( dbo.Owner.Owner_Type = 'C' ) AND  
         ( dbo.Customer.Customer_Type = 'WH' ) AND  
         ( dbo.Owner.Project_Id = :asProject ) AND  
         ( dbo.Customer.User_Field2 = :asWhCode ) )   
ORDER BY dbo.Owner.Owner_Cd ASC   

" arguments=(("asProject", string),("asWhCode", string)) )
