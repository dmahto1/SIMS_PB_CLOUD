table(column=(type=char(50) updatewhereclause=yes name=sku dbname="sku" )
 column=(type=char(20) updatewhereclause=yes name=supp_code dbname="supp_code" )
 column=(type=char(10) updatewhereclause=yes name=wh_code dbname="wh_code" )
 column=(type=char(1) updatewhereclause=yes name=inventory_type dbname="Inventory_Type" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=avail_qty dbname="Avail_Qty" )
 column=(type=char(4) updatewhereclause=yes name=uom_1 dbname="UOM_1" )
 column=(type=decimal(0) updatewhereclause=yes name=part_upc_code dbname="Part_UPC_code" )
 column=(type=char(255) updatewhereclause=yes name=user_field4 dbname="User_Field4" )
 retrieve="
select boh.sku, boh.supp_code, wh_code, Inventory_Type, sum(avail_qty) Avail_Qty, UOM_1, Part_UPC_code, User_Field4
from (SELECT cs.Project_id, cs.SKU, cs.supp_code, cs.wh_code, cs.inventory_type, cs.Avail_Qty + cs.alloc_qty + cs.tfr_in as avail_qty
	FROM  content_summary cs With (NOLOCK) 
	Where cs.project_id =:asproject and
    (cs.Avail_Qty + alloc_qty + tfr_in <> 0 ) 
    and ro_no<>'-'

union all

select dm.Project_Id, dp.sku, dp.Supp_Code,WH_Code,dp.Inventory_Type,dp.Quantity
from Delivery_Picking_Detail dp WITH(NOLOCK) inner join Delivery_Master DM WITH(NOLOCK) on dp.do_no=dm.do_no 
where dm.project_id=:asproject
and ord_status ='R'
and ro_no<>'-') as BOH
inner join item_master im With (NOLOCK) 
on boh.project_id = im.project_id and boh.supp_code = im.supp_code and boh.sku = im.sku
Group BY BOH.SKU, BOH.Supp_code, wh_code, inventory_type, uom_1, part_upc_code, user_field4
ORDER BY wh_code, Supp_code, inventory_type, SKU
" arguments=(("asproject", string)) )
