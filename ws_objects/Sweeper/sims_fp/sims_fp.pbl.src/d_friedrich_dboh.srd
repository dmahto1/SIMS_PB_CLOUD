table(column=(type=char(10) updatewhereclause=yes name=wh_code dbname="content_summary.wh_code" dbalias=".wh_code" )
 column=(type=char(50) updatewhereclause=yes name=sku dbname="content_summary.SKU" dbalias=".SKU" )
 column=(type=char(20) updatewhereclause=yes name=supp_code dbname="content_summary.Supp_code" dbalias=".Supp_code" )
 column=(type=char(1) updatewhereclause=yes name=inventory_type dbname="content_summary.inventory_type" dbalias=".inventory_type" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=avail_qty dbname="avail_qty" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=alloc_qty dbname="Alloc_Qty" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=sit_qty dbname="SIT_Qty" )
 column=(type=char(50) updatewhereclause=yes name=serial_no dbname="content_summary.serial_no" dbalias=".serial_no" )
 column=(type=char(4) updatewhereclause=yes name=uom_1 dbname="Item_Master.uom_1" dbalias=".uom_1" )
 retrieve="  SELECT content_summary.wh_code, 
		content_summary.SKU, 
         content_summary.Supp_code,
          content_summary.inventory_type, 
          sum(content_summary.Avail_Qty) as avail_qty,
         sum(content_summary.Alloc_Qty) as Alloc_Qty ,
		sum(content_summary.SIT_Qty) as SIT_Qty,
        	content_summary.serial_no,
		item_master.uom_1
FROM  content_summary With (NOLOCK) , item_master With (NOLOCK)
Where content_summary.project_id = :asProject and
		  content_summary.project_id =  item_master.project_id and
		 content_summary.sku =  item_master.sku and
		content_summary.supp_code =  item_master.supp_code and
		 ( Avail_Qty <> 0 or Alloc_Qty <> 0 ) and
		content_summary.SKU Not in (select sku from serial_Number_Inventory where project_id = 'FRIEDRICH') 
Group BY content_summary.wh_code, 
		content_summary.SKU, 
         content_summary.Supp_code,
         content_summary.wh_code, 
         content_summary.inventory_type, 
        	content_summary.serial_no,
		item_master.uom_1

Union	Select wh_code, 
				SKU, 
				'FAC' as supp_code, 
				'N' as Inventory_Type, 
				1 as Avail_Qty, 
				0 as Alloc_Qty, 
				0 as 'SIT_QTY', 
				Serial_No,
 				'EA' as uom_1
From  Serial_Number_Inventory
Where project_id = :asProject

ORDER BY content_summary.wh_code,
			  content_summary.SKU
		" arguments=(("asproject", string))  sort="wh_code A sku A " )
