table(column=(type=char(50) updatewhereclause=yes name=sku dbname="sku" )
 column=(type=char(8) updatewhereclause=yes name=ord_type dbname="ord_type" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=quantity dbname="Quantity" )
 column=(type=char(10) updatewhereclause=yes name=wh_code dbname="Wh_code" )
 column=(type=char(20) updatewhereclause=yes name=owner_cd dbname="owner_cd" )
 retrieve="SELECT 	a.sku,
			'RECEIVED' as ord_type, 
       	Sum(A.quantity) as Quantity, 
		 	B.Wh_code, 
			o.owner_cd 
	FROM 	receive_putaway A  With (NOLOCK)
			INNER JOIN receive_master B With (NOLOCK)  ON (A.ro_no = B.ro_no),
			owner o With (NOLOCK), 
			Item_master I With (NOLOCK)
	WHERE B.Wh_code 			= :as_whcode 	AND 
			B.project_id 		= :as_project 	AND
			o.owner_cd 			= :as_owner		AND
			b.Complete_Date 	>= :From_Date	AND 
			b.Complete_Date 	<= :To_Date		AND
         o.owner_id         = A.owner_id  AND
         o.project_id       = B.project_id AND	
			B.ord_status 		= 'C'  			and
			A.Sku = I.Sku 							and
			a.Supp_code = I.Supp_Code 			and
			b.Project_id = I.Project_ID 		AND
			a.sku = a.sku_Parent
Group By a.sku,
		 	B.Wh_code, 
			o.owner_cd

UNION ALL


SELECT 	a.sku,
			'PULLED' as ord_type, 
			Sum(A.quantity) as Quantity,
			B.wh_code, 
			o.owner_cd
	FROM 	delivery_picking A With (NOLOCK)
			INNER JOIN delivery_master B ON (A.do_no = B.do_no),
			owner o With (NOLOCK) , 
			Item_Master	I With (NOLOCK)
	WHERE B.Wh_code = :as_whcode 			AND 
			B.project_id = :as_project 	AND
			o.owner_cd 			= :as_owner	AND
			B.ord_status in ('C', 'D', 'R') and

			( (b.Carrier_notified_Date 	>= :From_Date	AND 
				b.Carrier_notified_Date 	<= :To_Date) or

				(b.Complete_date >= :From_Date	AND 
				b.Complete_date 	<= :To_Date	AND
				b.Carrier_notified_date is null) )		AND 

			o.owner_id         = A.owner_id  AND
         o.project_id       = B.project_id AND
			(a.sku = a.sku_Parent or a.Component_ind = 'W') AND
			a.SKU = I.SKU and
			a.Supp_Code = I.Supp_Code and
			B.Project_id = I.Project_ID
Group By a.sku,
		 	B.Wh_code, 
			o.owner_cd

UNION ALL


SELECT 	A.sku,
			'ADJUST' as ord_type, 
			sum(A.quantity - A.Old_quantity) as quantity, 
			A.wh_code, 
			o.owner_cd
	FROM 	adjustment A With (NOLOCK) ,
			owner o With (NOLOCK) ,
			Item_MASter	I With (NOLOCK)
	WHERE A.Wh_code 		= :as_whcode and 
			A.project_id 	= :as_project and
			o.owner_cd 			= :as_owner		AND
			o.owner_id         = A.owner_id  AND
         o.project_id       = A.project_id AND
			A.last_Update 	>= :From_Date	AND 
			A.LAst_Update 	<= :To_Date		  AND
			A.Project_ID = I.PRoject_ID AND
			A.SKU = I.SKU and
			A.Supp_Code = I.Supp_Code
Group By a.sku,
		 	a.Wh_code, 
			o.owner_cd



" arguments=(("as_project", string),("as_whcode", string),("as_owner", string),("from_date", string),("to_date", string))  sort="sku A ord_type A " )
