table(column=(type=char(50) updatewhereclause=yes name=item_master_sku dbname="SKU" )
 column=(type=char(20) updatewhereclause=yes name=item_master_supp_code dbname="Supp_Code" )
 column=(type=char(50) updatewhereclause=yes name=item_master_alternate_sku dbname="Alternate_SKU" )
 column=(type=decimal(5) updatewhereclause=yes name=total_shipped dbname="total_shipped" )
 retrieve="  SELECT Item_Master.SKU,   
         Item_Master.Supp_Code,   
         Item_Master.Alternate_SKU, 
			Sum(quantity) as total_shipped 
    FROM Delivery_Picking with (nolock),   
         Delivery_Master with (nolock),   
         Item_Master with (nolock) 
   WHERE ( Delivery_Master.DO_No = Delivery_Picking.DO_No ) and  
         ( Delivery_Master.Project_ID = Item_Master.Project_ID ) and  
         ( Item_Master.SKU = Delivery_Picking.SKU ) and  
         ( Item_Master.Supp_Code = Delivery_Picking.Supp_code ) and
			( Delivery_master.Ord_Status = 'C')
	Group By Item_MASter.Sku, Item_MASter.Supp_code, Item_MAster.Alternate_sku   
	having	Sum(quantity) > 0
" )
