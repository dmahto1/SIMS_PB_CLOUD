 retrieve="  SELECT dbo.Item_Forward_Pick.Project_ID,   
         dbo.Item_Forward_Pick.SKU,   
         dbo.Item_Forward_Pick.Supp_Code,   
         dbo.Item_Forward_Pick.WH_Code,   
         dbo.Item_Forward_Pick.L_Code,   
        isNull( dbo.Item_Forward_Pick.Min_FP_Qty,0) as Min_FP_Qty,   
        isNull( dbo.Item_Forward_Pick.Replenish_Qty,0) as Replenish_Qty,   
			dbo.Item_Forward_Pick.partial_pick_Ind,
			dbo.Item_Forward_Pick.Max_Qty_to_Pick, 
		isNull(dbo.Item_MaSter.Qty_2, 0) as Qty_2, 
     isNull( (Select Sum(avail_qty) from Content where dbo.Content.Project_ID = dbo.Item_Forward_Pick.Project_ID  and  
         			 dbo.Item_Forward_Pick.SKU = dbo.Content.SKU  and  
         			 dbo.Content.Supp_Code = dbo.Item_Forward_Pick.Supp_Code and dbo.Item_forward_pick.l_code <> Content.l_code) , 0 ) as c_avail_qty,
	 isNull( (Select Sum(avail_qty) from Content where dbo.Content.Project_ID = dbo.Item_Forward_Pick.Project_ID  and  
         			 dbo.Item_Forward_Pick.SKU = dbo.Content.SKU  and  
         			 dbo.Content.Supp_Code = dbo.Item_Forward_Pick.Supp_Code and dbo.Item_forward_pick.l_code = Content.l_code), 0 ) as c_curr_qty,
	isNull( (Select Sum(Quantity) from Transfer_master, Transfer_Detail where transfer_master.to_no = Transfer_detail.to_no and
						transfer_master.Project_id = :asProject and transfer_Master.s_warehouse = :asWarehouse and
						transfer_master.Ord_status in ('X', 'P') and Item_Forward_Pick.SKU = Transfer_detail.SKU and
						Item_Forward_Pick.Supp_code = Transfer_detail.Supp_code), 0 )  as c_pending_Qty

    FROM dbo.Item_Forward_Pick, 
			Item_Master 
   WHERE  dbo.Item_Forward_Pick.Project_id = :asProject and
			dbo.Item_Forward_Pick.wh_code = :asWarehouse and
			Item_Forward_Pick.Project_id = Item_MAster.Project_ID and
			Item_Forward_Pick.SKU = Item_MAster.SKU and 
			Item_Forward_Pick.Supp_Code = Item_MAster.Supp_Code
" arguments=(("asProject", string),("asWarehouse", string))  sort="c_replen_sort A        item_forward_pick_project_id A        item_forward_pick_wh_code A        item_forward_pick_sku A        item_forward_pick_supp_code A " )
