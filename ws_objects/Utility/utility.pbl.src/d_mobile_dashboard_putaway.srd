 retrieve="  SELECT dbo.Receive_Master.RO_No,   
         dbo.Receive_Master.Priority, 
         dbo.Receive_Master.Ord_Date,   
         dbo.Receive_Master.Request_Date,   
         dbo.Receive_Master.Ord_Status,   
         dbo.Receive_Master.Supp_Invoice_No,   
         dbo.Receive_Master.Mobile_User_Assigned,   
         dbo.Receive_Master.Mobile_Status_Ind,   
         dbo.Receive_Master.Mobile_Staging_Location,   
         dbo.Receive_Master.Mobile_Released_Time,   
         dbo.Receive_Master.Mobile_Putaway_Start_Time,   
         dbo.Receive_Master.Mobile_Putaway_Complete_Time,
		dateAdd( minute, :aiGreen, Mobile_Released_Time ) as Green_Release_Time, 	
		dateAdd( minute, :aiYellow, Mobile_Released_Time ) as Yellow_Release_Time, 
		dateAdd( minute, :aiRed, Mobile_Released_Time ) as Red_Release_Time, 
		~"      ~" as aging_Color    
    FROM dbo.Receive_Master   with(nolock)
	WHERE Project_id = :asProject 
	AND wh_code = :asWarehouse 
	AND mobile_Enabled_Ind = 'Y'
	AND (Ord_status Not in ('C','D') or (Ord_status in ('C','D') and complete_Date > :adtCompleteDate))
	

" update="dbo.Receive_Master" updatewhere=0 updatekeyinplace=no arguments=(("asProject", string),("asWarehouse", string),("adtCompleteDate", date),("aiGreen", number),("aiYellow", number),("aiRed", number),("asUpdate", string))  sort="mobile_status_ind A " )
