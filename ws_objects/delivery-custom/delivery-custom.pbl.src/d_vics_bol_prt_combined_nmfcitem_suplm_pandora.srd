table(column=(type=char(1) updatewhereclause=yes name=carton_count dbname="carton_count" )
 column=(type=char(10) updatewhereclause=yes name=carton_type dbname="Carton_Type" )
 column=(type=char(1) updatewhereclause=yes name=package_type dbname="Package_Type" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=item_qty dbname="item_qty" )
 column=(type=long updatewhereclause=yes identity=yes name=total_item_w dbname="total_item_w" )
 column=(type=char(12) updatewhereclause=yes name=hazard_tag dbname="hazard_tag" )
 column=(type=char(1104) updatewhereclause=yes name=hazmat_commodity_description dbname="hazmat_commodity_description" )
 column=(type=char(16) updatewhereclause=yes name=delivery_packing_do_no dbname="DO_No" )
 column=(type=char(50) updatewhereclause=yes name=delivery_packing_carton_no dbname="Carton_No" )
 column=(type=char(50) updatewhereclause=yes name=sku dbname="SKU" )
 column=(type=char(1) updatewhereclause=yes name=compute_0011 dbname="compute_0011" )
 column=(type=char(6) updatewhereclause=yes name=hazard_cd dbname="Hazard_Cd" )
 column=(type=char(67) updatewhereclause=yes name=sorting_cd dbname="Sorting_Cd" )
 retrieve="select ' ' as carton_count, 
	Max(DP.Carton_Type) as Carton_Type, 
	'' as Package_Type,
	Sum(DP.quantity) as item_qty,
	Cast( Round( Sum(DP.weight_gross *
		CASE WHEN DP.Standard_Of_Measure = 'M'
		THEN 2.2
		ELSE 1 END
		* DP.quantity) ,0) as INT) as total_item_w, 
	CASE WHEN Max(IM.Hazard_Cd) IS NOT NULL AND Rtrim(LTRIM(Max(IM.Hazard_Cd))) <> '' AND (Max(LU.Code_Id) is null or Max(LU.Code_Id) = '')
		THEN 'HAZARDOUS' 
		ELSE 'NONHAZARDOUS' END as hazard_tag,
	CASE WHEN Max(IM.Hazard_Cd) IS NOT NULL AND Rtrim(LTRIM(Max(IM.Hazard_Cd))) <> '' AND (Max(LU.Code_Id) is null or Max(LU.Code_Id) = '')
		THEN Max(IM.Hazard_Cd) + ',' + IsNull(Max(IM.Proper_Shipping_Name),'') + ',' + IsNull(Max(HM.Class),'') + ',' + IsNull(Max(HM.Packing_Group),'')
		ELSE
			CASE WHEN (Max(LU.User_Field2) is null or Max(LU.User_Field2) = '')
				THEN IsNull( Max(CC.Commodity_Description), ISNULL( Max( IM.User_Field5 ), '') ) 
				ELSE Max(LU.User_Field2)
			END
		END as hazmat_commodity_description,
	DP.DO_No, 
	DP.Carton_No, 
	DP.SKU, 
	Max(DP.Standard_Of_Measure),
	Max(IM.Hazard_Cd) as Hazard_Cd,
	RTRIM(LTRIM(DP.Do_No)) + RTRIM(LTRIM(DP.Carton_No)) + 
		CASE WHEN Max(IM.Hazard_Cd) IS NOT NULL AND Rtrim(LTRIM(Max(IM.Hazard_Cd))) <> '' AND (Max(LU.Code_Id) is null or Max(LU.Code_Id) = '')
		THEN 'H'
		ELSE 'N'
		END as Sorting_Cd
from Delivery_Packing DP
Inner Join 	Item_Master IM On IM.Project_Id = :as_project AND IM.SKU = DP.SKU
Left Outer Join Hazardous_Materials HM On IM.Hazard_Cd = HM.Identification_No and IM.Hazard_Cd_Suffix = HM.Hazard_Cd_Suffix
Left Outer Join	Commodity_Codes CC On IM.User_Field5 = CC.Commodity_Cd
Left Outer Join Lookup_table LU on LU.project_id = :as_project and LU.Code_Type = 'BOL_CHECK' and LU.Code_Descript = 'HAZARD_CD_EXCLUSION' and LU.Code_ID = DP.SKU
where DP.DO_No in
	(select do_no
	from delivery_master
	where consolidation_no = :as_conslNo)
group by DP.DO_No, DP.Carton_No, DP.sku
order by DO_No, carton_no
" arguments=(("as_project", string),("as_conslNo", string))  sort="sorting_cd A " )
