 retrieve="SELECT DISTINCT DM.DO_NO, DM.Cust_Name, DM.Cust_Code,  DM.Address_1, DM.Address_2, DM.Address_3, DM.Address_4, DM.City, DM.Country, 
awb_bol_no, DM.Invoice_No, left(Invoice_No, 6) SikaRef, DM.Carrier, DM.Delivery_Date, DM.Cust_Order_No, DM.User_Field4, 
DM.User_field6, P.Client_Name, P.Address_1, P.Address_2, P.Address_3, P.Address_4, P.City, P.State, P.Zip, P.Country,
WH.WH_Name, WH.Address_1, WH.Address_2, WH.Address_3, WH.Address_4, WH.City + ', ' + WH.State +' ' + WH.Zip WH_CityStateZip, WH.Country,
DM.State, DM.Zip, daa.address_1 BT_Addr1, daa.address_2 BT_Addr2, daa.address_3 BT_Addr3, daa.city +', ' + daa.state +' ' + daa.zip BT_CityStateZip,
DM.city +', ' + DM.state +' ' + DM.zip CityStateZip,
' ' as c_shipper, ' ' as c_route, 
dm.user_field7 as c_vehicle, ' ' as c_seal_no, 
0000.00 as c_cod_amnt, 0000.00 as c_cod_fee, ' ' as c_notes, ' ' as c_notes2, ' ' as c_notes3,
'Last User' as last_user, 'Last Update' as last_update, 
isnull(dm.complete_date, schedule_date) complete_date,
HZ.hazard_Text_Cd,
HazInd = case HZ.hazard_Text_Cd
  when 'NONHZRD' then ''
  else 'X'
end,
hazard_text,
IM.User_Field15 as Group_Desc,
sum(DP.Weight_Net * DP.quantity) as Weight,
UOM = case DP.standard_of_measure
  when 'E' then 'LBS' 
  else 'KG'
end,
sum(DP.Quantity) as Units,
0.00 as Rate,
IM.UOM_1, left(freight_class, 2) as Grp, im.user_field2 as ERG_No,
Freight_Terms

FROM Delivery_Master DM, 
  Project P,
  Warehouse WH,
  Delivery_Packing DP,   
  Item_Master IM,
  haz_mat_info HZ,
  delivery_alt_address DAA

WHERE P.Project_ID = DM.Project_ID and  
	WH.WH_Code = DM.WH_Code and  
	DM.Project_ID = :as_project_id AND  
   DM.awb_bol_no = :as_bol_no and
	DM.do_no = DP.do_no and    
	DP.Supp_Code = IM.Supp_Code and  
	DP.SKU = IM.SKU and  
	P.Project_id = HZ.Project_id and
	IM.hazard_text_cd = HZ.hazard_text_cd and
	P.Project_id = DAA.Project_id and
	DM.DO_NO = DAA.DO_NO

GROUP BY DM.cust_name, cust_code, DM.address_1, DM.address_2, DM.address_3, DM.address_4, 
DM.city, DM.country, awb_bol_no, invoice_no, carrier, DM.Delivery_Date, DM.Cust_Order_No,
DM.User_Field4, DM.User_field6, P.Client_Name, P.Address_1, P.Address_2, P.Address_3, P.Address_4, P.City, P.State, P.Zip, P.Country, 
WH.WH_Name, WH.Address_1, WH.Address_2, WH.Address_3, WH.Address_4, WH.City, WH.State, WH.Zip, WH.Country, DM.State, DM.Zip,			
DP.standard_of_measure, dm.complete_date, HZ.Hazard_Text_Cd, hazard_text, dm.user_field7,
daa.address_1, daa.address_2, daa.address_3, daa.city, daa.state, daa.zip, IM.UOM_1, im.grp, im.user_field2, Freight_Terms, schedule_date, im.user_field15, freight_class,
DM.DO_NO" arguments=(("as_project_id", string),("as_bol_No", string)) )
