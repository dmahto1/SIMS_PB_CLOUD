 retrieve="SELECT  '' rowfocusindicator,
	Receive_Putaway.Ro_No ,
	Receive_Putaway.SKU ,
	Item_Master.Description,
	Receive_Putaway.Supp_code ,
	Receive_Putaway.Owner_ID ,
	Receive_Putaway.Country_of_Origin ,
	Receive_Putaway.Line_Item_no, 
	Receive_Putaway.L_Code ,
	Receive_Putaway.Lot_No ,
	Receive_Putaway.Inventory_Type ,
	Receive_Putaway.Quantity ,
	1 as Serial_Qty,
	Receive_Putaway.PO_No ,
	Receive_Putaway.PO_No2 ,
	Receive_Putaway.Expiration_Date, 
	Receive_Putaway.container_ID, 
	Receive_Serial_Detail.Serial_No,  
	Receive_Serial_Detail.Id_No,
	Owner.Owner_Cd,
	Owner.Owner_Type,
	'                              ' as c_owner_name,
	Item_Master.Lot_Controlled_Ind , 
	Item_Master.PO_Controlled_Ind , 
	Item_Master.PO_NO2_Controlled_Ind,
	Item_Master.expiration_Controlled_Ind, 
	Item_Master.container_tracking_Ind,
	Item_Master.Serialized_Ind,
	Item_Master.QA_Check_Ind,
	Item_Master.GRP,
	Item_Master.Weight_1,  
	Item_Master.User_Field4 AS IM_User_Field4,
	Item_Master.User_Field14,
	Item_Master.User_Field18,
	Receive_Putaway.SKU_Parent ,
	Receive_Putaway.Component_Ind ,
	Receive_Putaway.Component_No, 
	Receive_Putaway.User_Line_Item_No, 
	Receive_Putaway.User_Field1,
	Receive_Putaway.User_Field2,
	Receive_Putaway.User_Field3,
	Receive_Putaway.User_Field4,
	Receive_Putaway.User_Field5,
	Receive_Putaway.User_Field6,
	Receive_Putaway.User_Field7,
	Receive_Putaway.User_Field8,
	Receive_Putaway.User_Field9,
	Receive_Putaway.User_Field10,
	Receive_Putaway.User_Field11,
	Receive_Putaway.User_Field12,
	Receive_Putaway.User_Field13,
	Receive_Putaway.Content_Record_Created_Ind
FROM Receive_Putaway with(nolock)
   INNER JOIN Receive_Master with (nolock) ON Receive_Master.RO_No = Receive_Putaway.RO_No
   INNER JOIN Item_Master with (nolock) ON Receive_Putaway.SKU = Item_Master.SKU
  			AND	Receive_Master.Project_Id =Item_Master.Project_Id 	AND Receive_Putaway.supp_code = Item_Master.Supp_Code
			AND Item_Master.Serialized_Ind IN ('Y','B')
	INNER JOIN Owner with (nolock)	ON Owner.Project_Id = Receive_Master.Project_Id
			AND Owner.Owner_Id = Receive_Putaway.Owner_Id
	LEFT OUTER JOIN Receive_Serial_Detail with(nolock) ON Receive_Putaway.Id_No = Receive_Serial_Detail.Id_No
			AND Receive_Putaway.Ro_No = Receive_Serial_Detail.Ro_No AND Receive_Serial_Detail.Ro_No =Receive_Master.Ro_No
			AND Receive_Putaway.SKU = Receive_Serial_Detail.SKU	AND Receive_Serial_Detail.SKU =Item_Master.SKU
WHERE Receive_Putaway.Ro_No = :Ro_No
	AND Receive_Master.Project_Id=:project_id" update="Receive_Serial_Detail" updatewhere=0 updatekeyinplace=yes arguments=(("ro_no", string),("project_id", string))  sort="line_item_no A sku A id_no A serial_no A " )
