table(column=(type=char(50) updatewhereclause=yes name=po_no2 dbname="Serial_Number_Inventory.Po_No2" dbalias=".Po_No2" )
 column=(type=char(25) updatewhereclause=yes name=carton_id dbname="Serial_Number_Inventory.Carton_Id" dbalias=".Carton_Id" )
 column=(type=char(50) updatewhereclause=yes name=serial_no dbname="Serial_Number_Inventory.Serial_No" dbalias=".Serial_No" )
 retrieve="  SELECT dbo.Serial_Number_Inventory.Po_No2,   
         dbo.Serial_Number_Inventory.Carton_Id,   
         dbo.Serial_Number_Inventory.Serial_No  
    FROM dbo.Serial_Number_Inventory  
   WHERE ( dbo.Serial_Number_Inventory.Po_No2 = :palletno ) AND  
         ( dbo.Serial_Number_Inventory.Project_Id = :projectid ) AND  
         ( dbo.Serial_Number_Inventory.WH_Code = :whcode ) AND  
         ( dbo.Serial_Number_Inventory.SKU = :sku ) AND  
         ( dbo.Serial_Number_Inventory.Carton_Id = :cartonid ) AND  
         ( dbo.Serial_Number_Inventory.l_code = :location ) AND  
         (( dbo.Serial_Number_Inventory.Serial_Flag = 'N' ) OR  
         ( dbo.Serial_Number_Inventory.Serial_Flag is NULL ) OR
		(   dbo.Serial_Number_Inventory.Serial_Flag = '' ) )
ORDER BY dbo.Serial_Number_Inventory.Po_No2 ASC,   
         dbo.Serial_Number_Inventory.Carton_Id ASC,   
         dbo.Serial_Number_Inventory.Serial_No ASC   
" arguments=(("projectid", string),("whcode", string),("sku", string),("palletno", string),("cartonid", string),("location", string))  sort="po_no2 A " )
