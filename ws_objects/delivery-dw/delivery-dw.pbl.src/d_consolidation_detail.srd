 retrieve="  SELECT ' ' as c_select_ind,  
			Delivery_MAster.Consolidation_no,  
         Delivery_Master.DO_No,   
         Delivery_Master.Project_ID,   
         Delivery_Master.Cust_Code,   
         Delivery_Master.Cust_Order_No,   
         Delivery_Master.Invoice_No,   
         Delivery_Master.Carrier,    
         Delivery_Master.Address_1,   
         Delivery_Master.Address_2,   
         Delivery_Master.City,   
         Delivery_Master.State,   
         Delivery_Master.Zip,   
         Delivery_Master.Ctn_Cnt,   
         Delivery_Master.Weight,   
         Delivery_Master.AWB_BOL_No,   
         Delivery_Master.Consolidate_To_WH_Code,   
         Delivery_Master.Ord_Date,   
         Delivery_Master.Ord_Status, 
			Delivery_MAster.Last_User, 
			Delivery_MASter.LAst_Update, 
			Delivery_MAster.Complete_Date, 
			Delivery_MAster.Carrier_Tracking_No, 
			Delivery_MAster.Ord_Type, 
			Delivery_MAster.User_Field4, 
			Delivery_MAster.Carrier_Notified_Date
    FROM Delivery_Master   
			Where Project_ID = :asProject and Consolidation_no = :asCONO and wh_code = :asWH
" update="Delivery_Master" updatewhere=0 updatekeyinplace=yes arguments=(("astype", string),("asProject", string),("asCONO", string),("asWH", string))  sort="consolidation_no A project_id A carrier A cust_code A user_field4 A " )
