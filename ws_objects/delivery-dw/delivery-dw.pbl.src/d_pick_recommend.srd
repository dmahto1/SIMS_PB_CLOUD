 column=(type=decimal(5) updatewhereclause=yes identity=yes name=c_avail_qty dbname="c_avail_qty" )
 column=(type=datetime updatewhereclause=yes name=arrival_date dbname="Arrival_Date" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=c_pick_amt dbname="c_pick_amt" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=owner_id dbname="Owner_ID" )
 column=(type=char(20) updatewhereclause=yes name=owner_owner_cd dbname="Owner_Cd" )
 column=(type=char(1) updatewhereclause=yes name=owner_owner_type dbname="Owner_type" )
 column=(type=char(30) updatewhereclause=yes name=inv_type_desc dbname="Inv_Type_desc" )
 column=(type=char(1) updatewhereclause=yes name=inventory_shippable_ind dbname="inventory_shippable_ind" )
 column=(type=datetime updatewhereclause=yes name=complete_date dbname="Complete_Date" )
 column=(type=decimal(0) updatewhereclause=yes name=shelf_life dbname="shelf_life" )
 retrieve="  SELECT Content.SKU,   
			Content.Supp_code, 
         Content.L_Code,   
			Content.Inventory_Type, 
        	Content.Country_of_Origin, 
         Content.Serial_No,   
         Content.Lot_No,   
			Content.PO_No,
			Content.PO_No2,
			Content.Container_ID,  
			Content.Expiration_Date,  
			Content.Component_no,
			Content.Cntnr_Length, 
			Content.Cntnr_Width, 
			Content.Cntnr_Height, 
			Content.Cntnr_Weight, 
			Sum(Content.Avail_Qty) as c_avail_qty,   
         Receive_Master.Arrival_Date,  
			0000000.00000 as c_pick_amt, 
			Owner.Owner_ID, 
			Owner.Owner_Cd, 
			Owner.Owner_type, 
			Inventory_Type.Inv_Type_desc, 
			Inventory_Type.inventory_shippable_ind,
			Receive_Master.Complete_Date,
			Item_Master.shelf_life   
    FROM Owner with (NoLock), Item_Master with (NoLock),
			Inventory_Type with (NoLock), 			
			Receive_Master with (NoLock)  right outer join Content with (NoLock)
			on Receive_Master.RO_No = Content.RO_No
   WHERE
			content.Project_id = :project_parm and
			content.wh_code = :warehouse_parm and
			content.sku = :sku_parm and 
			content.supp_code = :supplier_parm and
			Owner.Project_id = Content.Project_id and
			Owner.Owner_id = Content.Owner_ID and
			Content.Project_id = Inventory_type.Project_id and 
			Content.Inventory_Type = Inventory_Type.Inv_Type  and
			Content.sku =Item_Master.sku and
			Content.supp_code =Item_Master.supp_code and
			Content.Project_id =Item_Master.Project_id

Group By Content.L_Code,
			Content.Inventory_Type, 
			Content.SKU,
			Content.supp_code, 
			Content.Country_of_Origin, 
			Content.Serial_No,   
         Content.Lot_No, 
			Content.PO_No,
			Content.PO_No2, 
			Content.Container_ID, 
			Content.Expiration_Date, 
			Content.component_no,
			Content.Cntnr_Length, 
			Content.Cntnr_Width, 
			Content.Cntnr_Height, 
			Content.Cntnr_Weight, 
			Receive_Master.Arrival_Date, 
			Owner.Owner_ID, 
			Owner.Owner_Cd, 
			Owner.Owner_type, 
			Inventory_Type.Inv_Type_desc, 
			Inventory_Type.inventory_shippable_ind,
			Receive_Master.Complete_Date,
			Item_Master.shelf_life  
" arguments=(("project_parm", string),("warehouse_parm", string),("sku_parm", string),("supplier_parm", string))  sort="arrival_date A l_code A " )
