event clicked;
IF gs_project = "3COM_NASH" THEN
	This.m_13cootranslate.Visible=True
	This.m_14itemserial.Visible=True
ELSE
	This.m_13cootranslate.Visible=False
	This.m_14itemserial.Visible=False
END IF	


//IF gs_project = "POWERWAVE" THEN   //12-Mar-2013 :Madhu -Added to visible the itemserial for all projects
	This.m_14itemserial.Visible=True
//End If    //12-Mar-2013 :Madhu -Added to visible the itemserial for all projects

IF gs_project  =  "CHINASIMS" THEN
	m_23countryforceserialmatch.Visible = False
End If

//Jxlim 11/16/2010 adding Delivery State Routing for Comcast
IF gs_project = "COMCAST" THEN
	This.m_24deliverystaterouting.Visible=True
	This.m_24deliverystaterouting.Enabled=True
Else
	This.m_24deliverystaterouting.Visible=False
	This.m_24deliverystaterouting.Enabled=False
End If

//Jxlim 01/07/2011 Added HS code profile menu for Wine and Spirit
//Wine and Spirit is actually going to be multiple projects but all project will start with”WS-“  
//When you put in project specific logic you need to only reference the 1st thru 3rd fields instead of the whole project_id.  
String ls_project
ls_project = Left(gs_project,3)

If ls_project = 'WS-' Then	   
	This.m_25hscodeprofile.Visible=True
	This.m_25hscodeprofile.Enabled=True
Else
	This.m_25hscodeprofile.Visible=False
	This.m_25hscodeprofile.Enabled=False
End If


IF left(gs_project, 4) = "NIKE" THEN
	This.m_27serialprefix.Visible=True
End If


IF gs_project = "STRYKER" THEN
	This.m_28strykerwarehouse.Visible=True
	This.m_29strykermrpsku.Visible=True
End If


IF gs_project = "KLONELAB" THEN
	This.m_30customcartonbolconfig.Visible=True
End If

IF gs_project = "PANDORA" THEN
	This.m_31hazardousmaterials.Visible=True
	This.m_32advanceesdconfiguration.Visible=True
Else
	This.m_31hazardousmaterials.Visible=False
	This.m_32advanceesdconfiguration.Visible=False
End If

//Nov 2019 - MikeA - S38996: F18560 - Philips Blueheart NDD updates
//Dinesh - 11/23/2020- S51444- Philips-DA
IF gs_project = "PHILIPSCLS" or gs_project = "PHILIPS-DA"  THEN
	m_33holidayschedule.Visible = True
Else
	m_33holidayschedule.Visible = False
End If

gs_menuitem = Parent.classname( ) //05-MAy-2014 :Madhu- Added for open up User Manual upon F1 key.


end event

