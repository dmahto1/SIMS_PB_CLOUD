event clicked;//21-Apr-2014: Madhu- Open SIMS USER MANAUL when user press F1 - START
string lsUrl
inet iinet_base

CHOOSE CASE gs_menuitem
	CASE 'm_inbound'
			select User_Field1   into : lsUrl
			from dbo.Lookup_Table with (nolock)
			where project_id ='*ALL' and code_type ='MANUAL-INBOUND' and code_id ='WEB'
			using sqlca;
	CASE 'm_outbound'
			select User_Field1   into : lsUrl
			from dbo.Lookup_Table with (nolock)
			where project_id ='*ALL' and code_type ='MANUAL-OUTBOUND' and code_id ='WEB'
			using sqlca;
	CASE 'm_inventorymgmt'
			select User_Field1   into : lsUrl
			from dbo.Lookup_Table with (nolock)
			where project_id ='*ALL' and code_type ='MANUAL-INVMGT' and code_id ='WEB'
			using sqlca;
	CASE 'm_maintenance'
			select User_Field1   into : lsUrl
			from dbo.Lookup_Table with (nolock)
			where project_id ='*ALL' and code_type ='MANUAL-MAINTENANCE' and code_id ='WEB'
			using sqlca;
	CASE 'm_simple_edit','m_main'
			select User_Field1   into : lsUrl
			from dbo.Lookup_Table with (nolock)
			where project_id ='*ALL' and code_type ='MANUAL-REPORTS' and code_id ='WEB'
			using sqlca;
	CASE 'm_utilities'
			select User_Field1   into : lsUrl
			from dbo.Lookup_Table with (nolock)
			where project_id ='*ALL' and code_type ='MANUAL-UTILITY' and code_id ='WEB'
			using sqlca;
	END CHOOSE


GetContextService("Internet", iinet_base)
iinet_base.HyperlinkToURL(lsUrl)
//21-Apr-2014: Madhu- Open SIMS USER MANAUL when user press F1 - END
end event

