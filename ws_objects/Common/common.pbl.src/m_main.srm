event clicked;window wSheet
string wName
int sheetcount

wSheet = ParentWindow.GetFirstSheet()

IF IsValid(wSheet) THEN
    wName = wsheet.ClassName()
END IF

do while isvalid(wSheet)
	    sheetcount++
		wSheet = ParentWindow.GetNextSheet(wSheet)		 
loop

DateTime	ldtLogout

If gs_userid = "" Then
	
	if isvalid (w_sims_banner) and sheetcount = 1 then
		close(w_sims_banner)	//Sarun2014Apr09 Banner
	end if
	Open(w_login)
Else
	If sheetcount > 1 Then
		MessageBox("SIMS","Please Close all windows before logging off!")
		return
	End if
	if isvalid (w_sims_banner) and sheetcount = 1 then
		close(w_sims_banner)	//Sarun2014Apr09 Banner
	end if
		g.uf_logout()
End If

// 08/00 PCONKL
w_main.TriggerEvent("ue_set_menu")

end event

