end type

event clicked;str_parms lstrparms
String lsDoNo, lsPoNumber,lsuf13,lsuf21, lsConsolNo
inet	linit

gs_menuitem = Parent.classname( ) //05-MAy-2014 :Madhu- Added for open up User Manual upon F1 key.

//If f_check_access ("B3","") = 1 Then
	Choose Case upper(gs_project)
		
		Case "PHXBRANDS"
			OpenSheet(w_phx_brnds_uccs_labels,w_main, gi_menu_pos, Original!)		
		Case "LOGITECH"
			OpenSheet(w_logitech_shipping_labels,w_main, gi_menu_pos, Original!)
		Case "SCITEX-IID" // TAM 2006/10/23
			OpenSheet(w_scitex_shipping_labels,w_main, gi_menu_pos, Original!)
		Case "NYCSP"  //TAM 2007/05/13
			OpenSheet(w_generic_uccs_labels,w_main, gi_menu_pos, Original!)
		Case "INGRAM"  //TAM 2007/05/13
			OpenSheet(w_ingram_shipping_labels,w_main, gi_menu_pos, Original!)
		
		Case "STANLEY" /* 01/08 - PCONKL - using generic laser version */
			lstrparms.string_arg[1] = 'W_DOR'
			OpenSheetwithparm(w_ship_label, lstrparms, w_main,gi_menu_pos, Original!)
		
		Case "LAM-SG"
			OpenSheet(w_lam_shipping_labels,w_main, gi_menu_pos, Original!)	
		// pvh - NCR
		Case "NCR"
			OpenSheet(w_ncr_shipping_labels,w_main, gi_menu_pos, Original!)
		//Jxlim 06/25/2013 Replace current shipping label with NYX custom label		
		Case "NYX"			
			OpenSheet(w_nyx_sscc_labels,w_main, gi_menu_pos, Original!)
		CASE "KLONELAB" //MEA 09/12 - KloneLab			
			//We can only print based on DO_NO - User must have valid order open to pass DO_NO in from w_DO
			If isVAlid(w_do) Then
				if w_do.idw_main.RowCOunt() > 0 Then
					lsDoNo = w_do.idw_main.GetITemString(1,'do_no')
					lsuf13=Trim(Upper(w_do.idw_main.GetITemString(1,'user_field13')))
					lsuf21=Trim(Upper(w_do.idw_main.GetITemString(1,'user_field21')))
				End If
			End If
			
			If isNUll(lsDoNo) or  lsDoNo = '' Then
				Messagebox('Labels','You must have an order retrieved in the Delivery Order Window~rbefore you can print labels!')
			Else
				//If DM.UF13 = ‘Y’ then send to B2B for label printing
				//if (Trim(Upper(w_do.idw_main.GetITemString(1,'user_field13'))) = 'Y'  then //30-Oct-2014 :Madhu -commented
				if (lsuf13 ='Y' and lsuf21 = 'B2B')  then //30-Oct-2014 :Madhu- Added to call B2B for label printing, if DM.UF21 = 'B2B' and DM.UF13 =Y
					
					lsPoNumber = w_do.idw_main.GetITemString(1,'User_Field12')
					
					If IsNull(lsPoNumber) or Trim(lsPoNumber) =  '' Then
						
						MessageBox ("Error", "Client Cust Ref not specified. Unable to open brower for label.")
						
						Return
						
					End If
		
					//http://www2.b2bgateway.net/barcodes/WarehouseServices/KloneLab_Menlologin.asp?username=username&password=!@&ponumber=EXNULAO872709  
					GetContextService("Internet", linit)
					linit.HyperlinkToURL("http://www2.b2bgateway.net/barcodes/WarehouseServices/KloneLab_Menlologin.asp?username=menlolabel&password=!@label2012&ponumber="+lsPoNumber)
					
					UPDATE Delivery_Master Set DWG_UPLOAD = 'Y' Where Do_no = :lsDoNo;
					
				else //Otherwise open the baseline Shipping label window
					OpenSheet(w_generic_uccs_labels,w_main, gi_menu_pos, Original!)
				end if
			End If
// pvh - NCR

		Case "FRIEDRICH"			
			OpenSheet(w_friedrich_sscc_labels,w_main, gi_menu_pos, Original!)

		// TAM 2014/09/09
		Case "BOSCH"			
			OpenSheet(w_bosch_sscc_labels,w_main, gi_menu_pos, Original!)

		//04-Oct-2014 :Madhu-	ANKI- Carton SSCC Label -START
		Case "ANKI"
			 OpenSheet(w_anki_sscc_label_print, w_main,gi_menu_pos,Original!)
		//04-Oct-2014 :Madhu-	ANKI- Carton SSCC Label -END

		Case "H2O" /* 12/15 - PCONKL */
			 OpenSheet(w_h2o_labels, w_main,gi_menu_pos,Original!)
			 
		Case "GARMIN" /* 04/2016 - TAM - If Garmin Shipment order then use custom label otherwise use baseline */
			If isVAlid(w_do) Then
				if w_do.idw_main.RowCOunt() > 0 Then
					lsConsolNo = w_do.idw_main.GetITemString(1,'consolidation_no')
				End If
			else  
//				if isValid(w_batch_pick) Then
//					if w_batch_pick.idw_batch_master.RowCOunt() > 0 Then
//						lsConsolNo = w_batch_pick.idw_batch_master.GetITemString(1,'consolidation_no')
//					End If
//				End If
			End If
//			if w_do.idw_main.GetITemString(1,'consolidation_no') <> '' and not IsNull(w_do.idw_main.GetITemString(1,'consolidation_no')) Then
			if (lsConsolNo <> '' and not IsNull(lsConsolNo)) or isValid(w_batch_pick) Then
				OpenSheet(w_garmin_labels, w_main,gi_menu_pos,Original!)
			Else
				OpenSheet(w_generic_uccs_labels,w_main, gi_menu_pos, Original!)
			End If

		Case "KENDO" /* 04/16 - PCONKL */
			 OpenSheet(w_kendo_labels, w_main,gi_menu_pos,Original!)
			 
		Case "PANDORA" /* 07/16 - TAM */
// TAM -08/16 -  They are waiting to deploy the new Shipping Label in the future.  Until then they wanted a change to the generic shipping label
//			 OpenSheet(w_pandora_ship_label_print, w_main,gi_menu_pos,Original!)
			 OpenSheet(w_pandora_generic_shipping_labels, w_main,gi_menu_pos,Original!)
		Case Else
			// TAM 08/30/2007  Use Generic Label instead of window
			OpenSheet(w_generic_uccs_labels,w_main, gi_menu_pos, Original!)
			// pvh - 08/12/06
//			lstrparms.string_arg[1] = 'W_DOR'
//			OpenSheetwithparm(w_ship_label, lstrparms, w_main,gi_menu_pos, Original!)
//			OpenSheetwithparm(w_ship_label, "W_DOR", w_main,gi_menu_pos, Original!)
	End Choose
//End If


end event

