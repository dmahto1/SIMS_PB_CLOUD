type m_31hazardousmaterials from menu within m_6codetables
end type

event clicked;// Only those Pandora users identified in the Function Rights table will be granted access to this menu item
//  This is a departure of normal use of f_get_access() function that also authorizes Admin/Super Users/SuperDuperUsers

str_parms istr_parms
String ls_process = 'CODE_HM'
boolean lb_access = false
//Initialize string parms
istr_parms.String_arg[1] = '0'
istr_parms.String_arg[2] = '0'
istr_parms.String_arg[3] = '0'
istr_parms.String_arg[4] = '0'
istr_parms.String_arg[5] = '0'	 

if g.getFunctionRights(ls_process, 'E' ) then istr_parms.String_arg[1] = '1'
if g.getFunctionRights(ls_process, 'N' ) then istr_parms.String_arg[2] = '1'
if g.getFunctionRights(ls_process, 'D' ) then istr_parms.String_arg[3] = '1'
if g.getFunctionRights(ls_process, 'S' ) then istr_parms.String_arg[4] = '1'
if g.getFunctionRights(ls_process, 'C' ) then istr_parms.String_arg[5] = '1'

if  istr_parms.String_arg[1] = '1' or  istr_parms.String_arg[2]='1' or istr_parms.String_arg[3]='1' or istr_parms.String_arg[4]='1' or istr_parms.String_arg[5]='1' then lb_access=true

if lb_access then
	OpenSheetwithparm(w_maintenance_hazardous_materials,  istr_parms, w_main, gi_menu_pos, Original!)
Else
	MessageBox("Security Check", "You have no access to this function!",StopSign!)
End If

end event

