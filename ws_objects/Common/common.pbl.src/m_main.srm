event clicked;// 12/00 - PCONKL - Close the reports menu if another tab is selected
If isvalid(w_select_report) Then
	close(w_select_report)
End If

//don't show menu options unless authorzed by project

//Scanners
If g.is_scanner_ind = "Y" Then
	m_3assignscanner.Visible = True
Else
	m_3assignscanner.Visible = False
End If

//Batch Picking
If g.is_allow_Batch_Pick = "Y" Then
	
	m_batchpicking.Visible = True
	
	//08/06 - PCONKL - Can't have both Batch Picking and Delivery Order Open
	if isvalid(w_do) Then
		m_batchpicking.Enabled = False
	Else
		m_batchpicking.Enabled = True
	End If
	
	if isValid(w_batch_Pick) Then
		m_delivery_Order.Enabled = False
	Else
		m_delivery_Order.Enabled = True
	End If
	
Else
	m_batchpicking.Visible = False
End If

//Track & Trace
If g.ibTNTEnabled = True Then
	m_ShipmentsOutbound.Visible = True
	if g.ibEtaMaintEnabled then
		m_6EtaMaintenance.Visible = True
	else
		m_6EtaMaintenance.Visible = False
	end if
Else
	m_ShipmentsOutbound.Visible = False
	m_6EtaMaintenance.Visible = False
End If

//TimA Pandora issue #470
//Turn on Outbound Dashboard for OTM enabled projects.
If g.is_OTM_Enable_Ind = 'Y' then
	m_outbounddashboard.Visible = True
Else
	m_outbounddashboard.Visible = False	
End if

//Jxlim 07/20/2010 
If gs_project = 'COMCAST'  Then
	m_siklinecapture.Visible = True
Else
	m_siklinecapture.Visible = False
End IF


If gs_project = 'CHINASIMS' Then
	m_12.visible = False
End IF

if gs_project = 'PANDORA' then
	m_9loadtenderleadtime.Visible = TRUE
	m_10bol.visible = TRUE
else
	m_10bol.visible = FALSE
end if
end event

