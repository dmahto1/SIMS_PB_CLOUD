event clicked;//11-Sep-2015 :Madhu- As discussed in code review changed to provide F10 access. -START
//prompt authorization window to user (Accesslevel =1,2) to edit parent window.
Long llNewRow,lrtn,ll_row,ll_count
String lsFind

Datastore ldw_Authorize
Str_authorize strEC

SELECT Count(*) INTO :ll_count
FROM Lookup_Table with(nolock)
WHERE Project_ID = :gs_project AND
		Code_Type='F10_AUTHORIZE' AND
		User_Updateable_Ind ='Y' AND
		Code_ID = :gs_userid
USING SQLCA;

// 2019/09 - MikeA - S36602 - I2406 - Riverbed - SKU scanning validation
//Allow Admins per Boon Hee to access F10 Access. (Allow override sku scanning)

IF gs_project = 'RIVERBED' THEN
	If gs_role = "1"  Then
		f_method_trace_special( gs_project, this.ClassName() + ' - clicked', 'F10 Authorization SKU Override Validation for Following User# '+gs_userid,gs_system_no, ' ',' ',gs_system_no)
		m_unlock.visible = True
		m_unlock.enabled = True
		f_method_trace_special( gs_project, this.ClassName() + ' - clicked', 'Following Order# '+gs_system_no+ ' is Unlocked by User# ' +gs_userid,gs_system_no, ' ',' ',gs_system_no) 
		ParentWindow.TriggerEvent("ue_unlock")
		RETURN
	End IF
End If

string ls_TMS // //Dhirendra S53682  Google - SIMS - TMS F10 User Update -start
f_method_trace_special( gs_project, this.ClassName() + ' - clicked', 'F10 Authorization count is:  ' +string(ll_count)+' of Following User# '+gs_userid,gs_system_no, ' ',' ',gs_system_no) //07-Oct-2015  :Madhu added

IF (gs_role='1' or gs_role='2')  and (ll_count = 0) THEN
	
	f_method_trace_special( gs_project, this.ClassName() + ' - clicked', 'F10 Authorization screen is prompted for User# ' +gs_userid,gs_system_no, ' ',' ',gs_system_no) //07-Oct-2015  :Madhu added
	
	strEC.authorize_type='F10'
	OpenwithParm(w_authorization,strEC) //open authorization screen
	strEC =Message.powerobjectparm
	//  Dhirendra S53682  Google - SIMS - TMS F10 User Update -Start
	IF gs_project = 'PANDORA' and strEC.authorize_type = 'TMS' then 
		ls_TMS = strEC.authorize_type
	end if 
	strEC.authorize_type='F10'
	//   Dhirendra S53682  Google - SIMS - TMS F10 User Update -End
		IF strEC.nreturn = 1 THEN
		If Not isvalid(ldw_Authorize) then
			ldw_Authorize = Create datastore
			ldw_Authorize.Dataobject = 'd_authorize' 
			ldw_Authorize.SetTransObject(sqlca)
		End if
	
		ldw_Authorize.retrieve( ) //Retrieve the records, if any
		
		//If record already exists, allow to continue else make an entry
		lsFind ="Do_No ='"+gs_system_no+"' and Authorize_Type ='"+strEC.authorize_type+"'"
		ll_row =ldw_Authorize.find( lsFind, 1, ldw_Authorize.rowcount( ))
	
		// write the data to the fields
		If ll_row = 0 THEN
			llNewRow = ldw_Authorize.InsertRow(0)
			lrtn = ldw_Authorize.SetItem(llNewRow, 'Do_No', gs_system_no )
			lrtn = ldw_Authorize.SetItem(llNewRow, 'Authorize_Type', strEC.authorize_type )
			lrtn = ldw_Authorize.SetItem(llNewRow, 'UserId', strEC.Authorize_User )
			lrtn = ldw_Authorize.SetItem(llNewRow, 'license_no', strEC.Authorize_License )
			lrtn = ldw_Authorize.SetItem(llNewRow, 'Authorize_date', strEC.Authorize_Date )
			lrtn = ldw_Authorize.update( )	
		ELSE
			lrtn =1
		END IF
	ELSE
		lrtn = -1
	END IF
	
	//if authorization success then able to edit the parent window
	// Dhirendra S53682  Google - SIMS - TMS F10 User Update -Start
	IF lrtn >= 1  and ls_TMS <> 'TMS' THEN 
		f_method_trace_special( gs_project, this.ClassName() + ' - clicked', 'Following Order# '+gs_system_no+ ' is Unlocked by User# ' +strEC.Authorize_User,gs_system_no, ' ',' ',gs_system_no) //07-Oct-2015  :Madhu added
		ParentWindow.TriggerEvent("ue_unlock")
		MessageBox("Order Lock","Order is Un-locked")
	elseif  lrtn >= 1  and ls_TMS = 'TMS' then
		f_method_trace_special( gs_project, this.ClassName() + ' - clicked', 'Following Order# '+gs_system_no+ ' is Unlocked by User# ' +strEC.Authorize_User,gs_system_no, ' ',' ',gs_system_no) //07-Oct-2015  :Madhu added
		ParentWindow.TriggerEvent("ue_tms_unlock")
		MessageBox("Order Lock","Order is Un-locked")
		// Dhirendra S53682  Google - SIMS - TMS F10 User Update -END
	ELSE
		MessageBox("Order Lock","Order is locked")
	END IF
ELSE
	f_method_trace_special( gs_project, this.ClassName() + ' - clicked', 'Following Order# '+gs_system_no+ ' is Unlocked by User# ' +gs_userid,gs_system_no, ' ',' ',gs_system_no) //07-Oct-2015  :Madhu added
	ParentWindow.TriggerEvent("ue_unlock")
END IF

//Begin - 03/09/21- Dinesh - S54604 - SIMS -  Google - Unlock the Assign Pro Nbr field
IF gs_project = 'PANDORA' and ll_count >= 0 THEN
	f_method_trace_special( gs_project, this.ClassName() + ' - clicked', 'Following Order# '+gs_system_no+ ' is Unlocked by User# ' +gs_userid,gs_system_no, ' ',' ',gs_system_no) //07-Oct-2015  :Madhu added
	ParentWindow.TriggerEvent("ue_pro_unlock")
end if
//End - 03/09/21- Dinesh - S54604 - SIMS -  Google - Unlock the Assign Pro Nbr field

	
//11-Sep-2015 :Madhu- As discussed in code review changed to provide F10 access. -END
end event

