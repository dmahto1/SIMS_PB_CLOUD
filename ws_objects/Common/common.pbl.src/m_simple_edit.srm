event clicked;integer li_count
string  ls_process
boolean bF10_Okay,bF10Key=FALSE //11-Sep-2015 :Madhu- As discussed in code review changed to provide F10 access.
w_std_master_detail w_tmp

//Jxlim 04/12/2012 Commented out per Dave; we will check the accessiblity through gs_role instead of lookup table

////MEA - 04/09 - Use the Look-up table to store users who can "unlock" and modify values.
//IF Upper(gs_project) = "PANDORA" THEN
//	
//	SELECT Count(*) INTO :li_count
//		FROM Lookup_Table
//		WHERE Project_ID = 'PANDORA' AND
//				Code_Type = 'USER' AND 
//				Code_ID = :gs_userid
//		USING SQLCA;
//
//	IF SQLCA.SQLCode <> 0 THEN
//		
//		MessageBox ("DB Error", SQLCA.SQLErrText )
//		
//	END IF
//	
//	IF li_count > 0 then
//	
//		m_unlock.visible = true
//		//JXLIM 04/03/2012 Unlock F10
//		m_unlock.enabled = true
//		
//	ELSE
//		
//		m_unlock.visible = false
//		//JXLIM 04/03/2012 Unlock F10
//		m_unlock.enabled = False
//	END IF
//	
//ELSE
//	m_unlock.visible = false
//	
//END IF

//f_method_trace_special( gs_project, this.ClassName() + ' - clicked', 'F10 Pressed by User  '+gs_userid,gs_system_no, ' ',' ',gs_system_no) //07-Oct-2015  :Madhu added

//JXLIM 04/03/2012 Unlock F10 for super duper(-1) and super (0); user access is less than 1 BRD #386
If gs_role = "0"  OR gs_role = "-1" Then
	m_unlock.visible = True
	m_unlock.enabled = True

Else
	// ET3 2012-10-12: Pandora 356 enable F10 access selectively for operators (2)
	//SARUN2014NOV04 : Remove HardCoding of Pandora for Unlock Functionality and made it for Baseline
	If long(gs_role) <= 2 Then
		// check if user allowed to Edit and if user listed in trusted user lookup
		
		SELECT Count(*) INTO :li_count
			FROM Lookup_Table
			WHERE Project_ID = :gs_project AND
					//Code_Type = 'USER' AND //11-Sep-2015 :Madhu- As discussed in code review changed to provide F10 access.
					Code_Type='F10_AUTHORIZE' AND //11-Sep-2015 :Madhu- As discussed in code review changed to provide F10 access.
					User_Updateable_Ind ='Y' AND //11-Sep-2015 :Madhu- As discussed in code review changed to provide F10 access.
					Code_ID = :gs_userid
			USING SQLCA;
	
		IF SQLCA.SQLCode <> 0 OR li_count <> 1 THEN
			//MessageBox ("DB Error", SQLCA.SQLErrText )
			bF10_Okay = FALSE
		ELSE
			bF10_Okay = TRUE
		END IF
		
		w_tmp = ParentWindow
		ls_process = w_tmp.is_process
		
		IF KeyDown(KeyF10!) THEN bF10Key =TRUE //11-Sep-2015 :Madhu- As discussed in code review changed to provide F10 access.
		
		if g.getFunctionRights( ls_process , 'E' ) AND bF10_Okay then 
			m_unlock.visible = True
			m_unlock.enabled = True
		//11-Sep-2015 :Madhu- As discussed in code review changed to provide F10 access. -START
		//Added bF10Key condition to enable F10 access and validation done @ m_unlock and prompt a Authorization window
		elseif bF10Key then
			m_unlock.visible = True
			m_unlock.enabled = True
		//11-Sep-2015 :Madhu- As discussed in code review changed to provide F10 access. -END
		else
			//m_unlock.visible = False //11-Sep-2015 :Madhu- commented
			m_unlock.visible = True //11-Sep-2015 :Madhu- Added
			m_unlock.enabled = False
		end if
		
	Else
		m_unlock.visible = False
		m_unlock.enabled = False	
	End if

End If

// 2019/09 - MikeA - S36602 - I2406 - Riverbed - SKU scanning validation
//Allow Admins per Boon Hee to access F10 Access. (Allow override sku scanning)

IF gs_project = 'RIVERBED' THEN
	If gs_role = "1"  Then
		m_unlock.visible = True
		m_unlock.enabled = True
	End IF
End If

end event

