 retrieve="  SELECT    
         Holiday.Holiday_Name,   
         Holiday.Holiday_Date,   
         Holiday.Holiday_Country,   
         Holiday.Holiday_ID,
		Holiday.Project_ID,
		Holiday.Last_User,
		Holiday.last_update
    FROM Holiday WITH (NOLOCK)
	Where Holiday.Project_ID = :asProject  and
			Holiday.Holiday_Country = :asCounty and
			Year(Holiday.Holiday_Date) = :aiYear
ORDER BY Holiday.Holiday_Date 




" update="Holiday" updatewhere=0 updatekeyinplace=no arguments=(("asProject", string),("asCounty", string),("aiYear", number))  sort="holiday_date A " )
