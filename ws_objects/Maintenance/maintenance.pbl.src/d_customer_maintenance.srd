 retrieve="  SELECT Customer.Project_ID,   
         Customer.Cust_Code,   
         Customer.Cust_Name,   
         Customer.Address_1,   
         Customer.Address_2,   
         Customer.Address_3,   
         Customer.Address_4,   
         Customer.City,   
         Customer.Country,   
         Customer.Contact_Person,   
         Customer.Tel,   
         Customer.Fax,    
         Customer.Remark,      
         Customer.Priority,   
         Customer.User_Field1,   
         Customer.User_Field2,   
         Customer.User_Field3,  
			Customer.User_Field4, 
			Customer.User_Field5,   
			Customer.User_Field6, 
			Customer.User_Field7, 
			Customer.User_Field8,  
			Customer.User_Field9, 
			Customer.User_Field10, 
         Customer.Last_User,   
         Customer.Last_Update,   
         Customer.Price_Class,   
         Customer.Tax_Class,     
         Customer.Discount,   
         Customer.Email_Address,     
         Customer.State,   
         Customer.Zip,   
         Customer.Customer_Type, 
			CCC_Enabled_Ind,
			Customer.Vat_ID,
		     Customer.Allow_containers_Ind     
    FROM Customer  
   WHERE ( Customer.Project_ID = :pid ) AND  
         ( Customer.Cust_Code = :code )    
" update="Customer" updatewhere=0 updatekeyinplace=yes arguments=(("pid", string),("code", string)) )
