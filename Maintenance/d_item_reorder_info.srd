 retrieve="  SELECT Reorder_Point.Project_ID,   
         Reorder_Point.SKU,   
         Reorder_Point.WH_Code,   
         Reorder_Point.MAX_Supply_Onhand,   
         Reorder_Point.MIN_ROP,   
         Reorder_Point.Reorder_Qty,   
         Reorder_Point.Supp_Code,   
         Reorder_Point.owner_id,    
         Owner.Owner_Type,   
         Owner.Owner_Cd,
			Reorder_Point.PONO  
    FROM Reorder_Point
    LEFT OUTER JOIN Owner
      ON Reorder_Point.Project_ID = Owner.Project_ID  
     AND Reorder_Point.owner_id = Owner.Owner_ID
   WHERE ( ( Reorder_Point.Project_ID = :project ) AND  
         ( Reorder_Point.SKU = :sku ) AND  
         ( Reorder_Point.Supp_Code = :supplier ) )    
" update="Reorder_Point" updatewhere=0 updatekeyinplace=yes arguments=(("project", string),("sku", string),("supplier", string)) )
