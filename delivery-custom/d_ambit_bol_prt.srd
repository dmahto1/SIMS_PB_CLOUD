 retrieve="SELECT DISTINCT          
		a.Cust_Name,   
         a.Address_1,   
         a.Address_2,   
         a.Address_3,   
         a.Address_4,   
         a.City,   
         a.Country,   
         a.Invoice_No,   
         a.Carrier,   
         a.Delivery_Date,   
         a.Cust_Order_No,   
         a.User_Field4, 
			a.Tel,
			a.AWB_BOL_No, 
		Project.Client_Name,   
         Project.Address_1,   
         Project.Address_2,   
         Project.Address_3,   
         Project.Address_4,   
         Project.City,   
         Project.State,   
         Project.Zip,   
         Project.Country,   
         Project.Remark,
			Project.Tel,  			
         Warehouse.WH_Name,   
         Warehouse.Address_1,   
         Warehouse.Address_2,   
         Warehouse.Address_3,   
         Warehouse.Address_4,   
         Warehouse.City,   
         Warehouse.State,   
         Warehouse.Zip,   
         Warehouse.Country,  
			Warehouse.Tel, 
         Customer.Cust_Name,
			Customer.Address_1, 
			Customer.Address_2,
			Customer.Address_3,
			Customer.Address_4,
			Customer.City,
			Customer.State,
			Customer.Zip,
			Customer.Country,
			Customer.Tel,
			space(40) as rem_Cust_Name,
			space(40) as rem_Address_1, 
			space(40) as rem_Address_2,
			space(40) as rem_Address_3,
			space(40) as rem_Address_4,
			space(30) as rem_City,
			space(30) as rem_State,
			space(15) as rem_Zip,
			space(30) as rem_country,
			space(20) as tel_remit,
			a.State,   
         	a.Zip,			
			Customer.Customer_Type,
			c.Code_ID,
			c.Code_Descript , 
			'                    ' as c_shipper, 
			'                                                                                         ' as c_route, 
			'                                    ' as c_vehicle, 

			0000 as c_pieces,    
         0000.00 as c_weight,   
			0000.00 as c_rate,   
			0000.00 as c_charges, 

			0000 as c_pieces2,    
			'                                 ' as c_desc2,
         0000.00 as c_weight2,   
			0000.00 as c_rate2,   
			0000.00 as c_charges2, 

			0000 as c_pieces3,    
			'                                 ' as c_desc3,
         0000.00 as c_weight3,   
			0000.00 as c_rate3,   
			0000.00 as c_charges3, 

			0000 as c_pieces4,    
			'                                 ' as c_desc4,
         0000.00 as c_weight4,   
			0000.00 as c_rate4,   
			0000.00 as c_charges4, 

			0000 as c_pieces5,    
			'                                 ' as c_desc5,
         0000.00 as c_weight5,   
			0000.00 as c_rate5,   
			0000.00 as c_charges5, 

			0000.00 as c_cod_amnt,     
			0000.00 as c_cod_fee, 
			'Last User' as last_user, 
			'Last Update' as last_update    
FROM 
         Project,   
         Warehouse,
         Lookup_Table b  right outer join delivery_master a
		 on b.Project_ID = a.Project_Id,				
	     Lookup_Table c right outer join Customer
		 on c.Code_ID = Customer.Customer_Type 	
		 		
WHERE ( Project.Project_ID = a.Project_ID ) and  
      ( Warehouse.WH_Code = a.WH_Code ) and  
		Customer.Project_ID = a.Project_ID and 
		Customer.Cust_Code = :as_cust_code AND			
      (c.Code_Type  = 'CTYPE') and			
      ( a.Project_ID = :as_project_id ) AND  
      ( a.do_no = :as_dono ) " retrieve.asneeded=yes arguments=(("as_project_id", string),("as_dono", string),("as_cust_code", string)) )
