table(column=(type=char(20) updatewhereclause=yes name=ordernumber dbname="OrderNumber" )
 column=(type=datetime updatewhereclause=yes name=createdate dbname="CreateDate" )
 column=(type=decimal(5) updatewhereclause=yes name=expectedquantity dbname="ExpectedQuantity" )
 column=(type=char(20) updatewhereclause=yes name=requestor dbname="Requestor" )
 column=(type=char(1) updatewhereclause=yes name=status dbname="Status" )
 column=(type=datetime updatewhereclause=yes name=completedate dbname="CompleteDate" )
 column=(type=decimal(0) updatewhereclause=yes name=doshipcnt dbname="DOShipCnt" )
 column=(type=decimal(0) updatewhereclause=yes name=pandoralinenumber dbname="PandoraLineNumber" )
 column=(type=char(50) updatewhereclause=yes name=gpn dbname="GPN" )
 column=(type=decimal(5) updatewhereclause=yes name=shipppedquantity dbname="ShipppedQuantity" )
 column=(type=decimal(5) updatewhereclause=yes name=backorderquantity dbname="BackorderQuantity" )
 column=(type=long updatewhereclause=yes identity=yes name=orderageindays dbname="OrderAgeInDays" )
 column=(type=datetime updatewhereclause=yes name=now dbname="Now" )
 column=(type=datetime updatewhereclause=yes name=bodate dbname="BODate" )
 column=(type=decimal(0) updatewhereclause=yes name=boshipcnt dbname="BOShipCnt" )
 column=(type=char(16) updatewhereclause=yes name=boorderno dbname="BOOrderNo" )
 column=(type=char(1) updatewhereclause=yes name=backorderstatus dbname="BackorderStatus" )
 column=(type=decimal(0) updatewhereclause=yes name=dobackorderno dbname="DOBackorderNo" )
 column=(type=char(16) update=yes updatewhereclause=yes name=do_no dbname="DO_No" )
 column=(type=char(25) updatewhereclause=yes name=supervisorapproved dbname="SupervisorApproved" )
 column=(type=datetime updatewhereclause=yes name=approveddate dbname="ApprovedDate" )
 retrieve="select dm.Invoice_No OrderNumber,dm.ord_date CreateDate,dd.Req_Qty ExpectedQuantity
	,o.Owner_Cd Requestor,dm.ord_status Status,dm.complete_date CompleteDate,dm.ship_cnt DOShipCnt
	,dd.Line_Item_No PandoraLineNumber,dd.sku GPN,dd.Alloc_Qty ShipppedQuantity
	,bd.Req_Qty BackorderQuantity, ISNULL(DATEDIFF(dd,bo.Ord_Date,GETDATE()),0) OrderAgeInDays
	,GETDATE() Now,bo.Ord_Date BODate,bo.ship_cnt BOShipCnt,bo.do_no as BOOrderNo,bo.Ord_Status as BackorderStatus
	,dm.Back_Order_No DOBackorderNo, auth.DO_No,auth.UserId SupervisorApproved,Authorize_Date ApprovedDate
from Authorized_Signer auth
LEFT OUTER JOIN delivery_master dm 
	ON dm.DO_No = auth.DO_No and dm.Project_Id = :as_project
LEFT OUTER JOIN delivery_detail dd
	ON dd.DO_No = auth.DO_No 
		and dd.Req_Qty > dd.Alloc_Qty
LEFT OUTER JOIN Owner o
	ON o.Owner_Id = dd.Owner_Id
LEFT OUTER JOIN delivery_master bo
	ON bo.project_id = :as_project and bo.Invoice_No = dm.Invoice_No 
		and bo.Ship_Cnt = ISNULL(dm.ship_cnt,0) + 1
LEFT OUTER JOIN delivery_detail bd
	ON bd.DO_No = bo.DO_No
		and bd.Line_Item_No = dd.Line_Item_No
where auth.authorize_type = 'SS' " arguments=(("as_project", string)) )
