 retrieve="SELECT  dbo.Delivery_Master.DO_No ,     
			dbo.Delivery_Master.Project_ID ,       
			dbo.Delivery_Master.Ord_Date,      
			dbo.Delivery_Master.Schedule_Date ,        
			dbo.Delivery_Master.Request_Date ,      
			dbo.Delivery_Master.Ord_Status ,
			dbo.Delivery_Master.Ord_Type,        
			dbo.Delivery_Master.Wh_Code ,           
			dbo.Delivery_Master.Cust_Order_No ,    
			dbo.Delivery_Master.Cust_Code ,       
			dbo.Delivery_Master.Cust_Name ,      
			dbo.Delivery_Master.Address_1 ,      
			dbo.Delivery_Master.City ,       
			dbo.Delivery_Master.State ,         
			dbo.Delivery_Master.Zip , 
			 dbo.Delivery_Master.Country ,           
			dbo.Delivery_Master.Invoice_No,
			dbo.Delivery_Master.Shipping_Instructions,
			dbo.Delivery_Master.Carrier,
			(Select Sum(dbo.Delivery_Detail.Req_Qty) from  Delivery_Detail with (nolock) where dbo.Delivery_Master.Do_No = dbo.Delivery_Detail.Do_No) As Req_Qty ,
			(Select Sum(dbo.Delivery_Detail.Alloc_Qty) from  Delivery_Detail with (nolock) where dbo.Delivery_Master.Do_No = dbo.Delivery_Detail.Do_No) As Alloc_Qty 
    FROM dbo.Delivery_Master  with (nolock)
WHERE ( dbo.Delivery_Master.Ord_Status not in ('C', 'V') )

" update="dbo.Delivery_Master" updatewhere=1 updatekeyinplace=no )
