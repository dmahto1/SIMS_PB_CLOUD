 retrieve="SELECT   Delivery_Master.DO_No,
			Delivery_Master.Invoice_No,
		   Delivery_Master.Ord_Status,
           IsNull((Select count( DISTINCT carton_no  )  From Delivery_Packing Where Delivery_Packing.Do_No =  Delivery_Master.Do_No and Delivery_Packing.carton_no is not null and ltrim(Delivery_Packing.carton_no) <> '' ), 0) AS Ctn_Cnt,   
         Delivery_Master.Remark,    
         cust_name as cust_name, 
         address_1 as shipadd1, 
			Delivery_Master.User_Field16 as maddress1
			FROM Delivery_Master    
WHERE   (Delivery_Master.Project_ID = :project_id ) AND 
			 (Delivery_Master.schedule_date >= :date1  AND  
          Delivery_Master.schedule_date <= :date2 ) AND  
          Delivery_Master.WH_Code = :whcode  AND delivery_Master.ord_status <> 'H' and 
          isnull(Delivery_Master.Carrier,'') like :transport AND
          isnull(delivery_master.User_Field13,'') like :as_region And
			  Delivery_Master.ord_status  like :as_status



" arguments=(("project_id", string),("date1", datetime),("date2", datetime),("whcode", string),("transport", string),("as_region", string),("as_status", string)) )
