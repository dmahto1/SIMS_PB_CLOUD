 retrieve="  SELECT Distinct
			' ' as c_print_ind,
			 dbo.Delivery_Detail.Req_Qty As No_of_Copies,
			 dbo.Delivery_Detail.Alloc_Qty  As QTY,
			(Select  Count(Distinct Carton_No) FROM Delivery_Packing Where  Delivery_Packing.DO_No = Delivery_Master.DO_No) As Total_Cartons ,

			 dbo.Delivery_Detail.Line_Item_No,
			rtrim(dbo.Delivery_Detail.Sku) as sku,
			'                                                                                       ' As Label,
			rtrim(dbo.Delivery_Detail.Customer_Sku) as Cust_sku,
			rtrim(dbo.Item_Master.User_Field4) as Cust_Size,
			rtrim(dbo.Delivery_Detail.Customer_Sku) + ' ' + rtrim(dbo.Item_Master.User_Field4)  as Cust_sku_size,
			rtrim(dbo.Delivery_Detail.SKU) + '/' + rtrim(dbo.Item_Master.Description) + '/' + rtrim(dbo.Item_Master.User_Field1) as Klone_sku_desc_color,
			rtrim(left(dbo.Delivery_Detail.Line_Item_Notes,17)) As Desc1,
			rtrim(SubString(dbo.Delivery_Detail.Line_Item_Notes,18,34)) As Desc2,
			dbo.Delivery_Detail.Price As Price,
			rtrim(dbo.Item_Master.Part_UPC_Code) as Upc,
			rtrim(dbo.Item_Master.User_Field5) as Dept,
			rtrim(dbo.Delivery_Detail.User_Field1) as Class,
			rtrim(dbo.Item_Master.Description) as Description,
			rtrim(dbo.Delivery_Detail.User_Field2) as Season,
			rtrim(dbo.Delivery_Detail.User_Field5) as Vendor_style,
			rtrim(dbo.Item_Master.User_Field1) as Color,
			rtrim(dbo.Item_Master.User_Field4) as Size,
			rtrim(dbo.Delivery_Detail.User_Field8) as Shoebox_nbr,
			rtrim(dbo.Delivery_Detail.User_Field6) as Style ,


			rtrim(dbo.Delivery_Master.Cust_Order_No) AS cust_order_no ,
			
			rtrim(dbo.Warehouse.Address_1) AS Addr1 ,
			rtrim(dbo.Warehouse.Address_2) AS Addr2 ,
			rtrim(dbo.Warehouse.Address_3) AS Addr3 ,
			rtrim(dbo.Warehouse.City) + SPACE(1)+ ',' + SPACE(1) + rtrim(dbo.Warehouse.State)+ SPACE(1)+ rtrim(dbo.Warehouse.Zip) as Wh_Zip ,
		
			rtrim(	dbo.Delivery_Master.ord_type) as ord_type,
			rtrim(	dbo.Delivery_Master.Cust_Name) as cust_name ,
			rtrim(dbo.Delivery_Master.Address_1) AS Cust_Addr1 ,
			rtrim(dbo.Delivery_Master.Address_2) AS cust_Addr2 ,
			rtrim(dbo.Delivery_Master.Address_3) AS cust_Addr3 ,
			rtrim(dbo.Delivery_Master.City) + SPACE(1)+ ',' + SPACE(1) + rtrim(dbo.Delivery_Master.State)+ SPACE(1)+ rtrim(dbo.Delivery_Master.Zip) as cust_zip   ,
			rtrim(dbo.Delivery_Master.City) as city ,			
			
			rtrim(dbo.Item_Master.Country_Of_Origin_Default) as Country_Of_Origin ,	
			(Select  Distinct  TOP 1 Length FROM Delivery_Packing Where  Delivery_Packing.DO_No = Delivery_Master.DO_No) As length ,
			(Select  Distinct TOP 1 Width FROM Delivery_Packing Where  Delivery_Packing.DO_No = Delivery_Master.DO_No) As Width,
			(Select  Distinct TOP 1 Height FROM Delivery_Packing Where  Delivery_Packing.DO_No = Delivery_Master.DO_No) As Height,
			(Select  Distinct  TOP 1 Weight_Gross FROM Delivery_Packing Where  Delivery_Packing.DO_No = Delivery_Master.DO_No) As Weight_Gross ,


			rtrim(dbo.Delivery_Master.User_Field12) po,	
			rtrim(dbo.Delivery_Master.User_Field20) Carrier_load
		
			
			

    FROM dbo.Delivery_Master WITH (NOLOCK) ,   
         dbo.Delivery_Detail WITH (NOLOCK),   
         dbo.Item_Master WITH (NOLOCK) ,
		dbo.Warehouse WITH (NOLOCK)
   WHERE ( dbo.Delivery_Detail.DO_No = dbo.Delivery_Master.DO_No ) and  
         ( dbo.Delivery_Detail.Supp_Code = dbo.Item_Master.Supp_Code ) and  
		( dbo.Delivery_Master.WH_Code = dbo.Warehouse.WH_Code) AND 
         ( dbo.Delivery_Detail.SKU = dbo.Item_Master.SKU ) and  
         ( dbo.Delivery_Master.Project_ID = dbo.Item_Master.Project_ID )    and
			Delivery_Master.do_no = :asDONO  and
			 Delivery_Master.Project_ID = 'KLONELAB'

order by dbo.Delivery_Detail.Line_Item_No

	" arguments=(("asDONO", string)) )
