 retrieve="  SELECT Distinct ' ' as c_select_Ind,  
			dbo.Delivery_MAster.Project_ID, 
			dbo.Delivery_MAster.wh_Code,  
			dbo.Carrier_Master.Trax_Carrier_Code, 
			Max(dbo.Trax_eod.To_date) as last_eod_date, 
			Max(dbo.Trax_eod.batch_id) as last_eod_batchID, 
			Max(dbo.Delivery_MAster.Complete_Date) as max_complete_Date,  
			Case When  Max(dbo.Trax_eod.To_date) is null Then DateAdd(D,-1,Max(dbo.Delivery_MAster.Complete_Date)) Else DateAdd(S,1,Max(dbo.Trax_eod.To_date)) End as Begin_EOD_DAte,
			Max(dbo.Delivery_MAster.Complete_Date)  as next_eod_date
    FROM dbo.Carrier_Master LEFT OUTER JOIN dbo.Trax_EOD ON dbo.Carrier_Master.Project_ID = dbo.Trax_EOD.Project_ID AND dbo.Carrier_Master.Trax_Carrier_Code = dbo.Trax_EOD.Carrier,   
           dbo.Delivery_Master
   WHERE ( dbo.Delivery_Master.Project_ID = dbo.Carrier_Master.Project_ID ) and  
         ( dbo.Delivery_Master.Carrier = dbo.Carrier_Master.Carrier_Code ) and
			delivery_MAster.Project_ID = :Project and
			delivery_MAster.wh_Code = :Warehouse and
			Trax_eod.wh_Code = :Warehouse and
			carrier_MAster.Project_ID = :Project and 
			carrier_master.Trax_carrier_Code > ''
Group by dbo.Delivery_MAster.Project_ID, 
			dbo.Delivery_MAster.wh_Code, 
			dbo.Carrier_Master.Trax_Carrier_Code

Having (Max(dbo.Delivery_MAster.Complete_Date) > Max(dbo.Trax_eod.To_date) ) or (Max(dbo.Trax_eod.To_date) is null and Max(dbo.Delivery_MAster.Complete_Date) > DateAdd(D,-7,GetDate())) or
			Max(dbo.Trax_eod.To_date) >  DateAdd(D,-7,GetDate())

