table(column=(type=char(50) update=yes updatewhereclause=yes name=sku dbname="SKU" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=owner_id dbname="Owner_ID" )
 column=(type=char(20) updatewhereclause=yes name=owner_cd dbname="owner_cd" )
 column=(type=char(50) update=yes updatewhereclause=yes name=po_no dbname="PO_No" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=sys_qty dbname="Sys_Qty" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=result_1 dbname="Result_1" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=result_2 dbname="Result_2" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=result_3 dbname="Result_3" )
 retrieve="SELECT distinct CC_Inventory.SKU,
CC_Inventory.Owner_ID,
owner.owner_cd,   
CC_Inventory.PO_No, 
SUM(CC_Inventory.Quantity) as Sys_Qty,    
SUM(IsNull(CC_Result_1.Quantity,0)) as Result_1,   
SUM(IsNull(CC_Result_2.Quantity,0)) as Result_2,   
SUM(IsNull(CC_Result_3.Quantity,0)) as Result_3
FROM CC_Inventory With (NOLOCK)  
INNER JOIN CC_Master With (NOLOCK) ON CC_Master.cc_no = CC_Inventory.cc_no
LEFT JOIN Owner With (NOLOCK) ON CC_Inventory.owner_id = owner.owner_id
LEFT JOIN CC_Result_1 With (NOLOCK) ON CC_Inventory.CC_No = CC_Result_1.CC_No
AND CC_Inventory.line_item_no = CC_Result_1.line_item_no
LEFT JOIN CC_Result_2 With (NOLOCK) ON CC_Inventory.CC_No = CC_Result_2.CC_No
AND CC_Inventory.line_item_no = CC_Result_2.line_item_no
LEFT JOIN CC_Result_3 With (NOLOCK) ON CC_Inventory.CC_No = CC_Result_3.CC_No
AND CC_Inventory.line_item_no = CC_Result_3.line_item_no
WHERE CC_Inventory.cc_no = :a_order
Group By CC_Inventory.SKU, CC_Inventory.Owner_ID, owner.owner_cd,  CC_Inventory.PO_No
" arguments=(("a_order", string))  sort="sku A owner_id A po_no A " )
