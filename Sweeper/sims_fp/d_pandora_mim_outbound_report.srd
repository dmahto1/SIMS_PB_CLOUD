table(column=(type=char(10) updatewhereclause=yes name=wh_code dbname="WH_Code" )
 column=(type=char(50) updatewhereclause=yes name=invoice_no dbname="Invoice_No" )
 column=(type=char(1) updatewhereclause=yes name=ord_type dbname="Ord_Type" )
 column=(type=datetime updatewhereclause=yes name=ord_date dbname="Ord_Date" )
 column=(type=char(1) updatewhereclause=yes name=ord_status dbname="Ord_Status" values="Confirming	Z/Hold	H/New	N/Process	P/Picking	I/Packing	A/Delivered	D/Completed	C/Void	V/Ready	R/QA Check	Q/" )
 column=(type=datetime updatewhereclause=yes name=delivery_date dbname="Delivery_Date" )
 column=(type=datetime updatewhereclause=yes name=receive_date dbname="Receive_Date" )
 retrieve="SELECT  Delivery_Master.WH_Code,   
         Delivery_Master.Invoice_No,   
         'O' as Ord_Type,
         Delivery_Master.Ord_Date as Ord_Date,   
         Delivery_Master.Ord_Status,   
         Delivery_Master.Delivery_Date as Delivery_Date,
         null as Receive_Date
FROM Delivery_Master with (nolock)
WHERE ( Delivery_Master.Project_Id = :as_project ) 
and ((Ord_Status not in('c', 'D') and Delivery_Master.Ord_Date>= getdate()-60) Or (Ord_Status in('C', 'D') and Complete_Date > getdate()-10))
AND (Delivery_Master.EDI_Batch_Seq_No is not null)
AND (Delivery_Master.EDI_Batch_Seq_No IN
	(SELECT EDI_Outbound_Header.EDI_Batch_Seq_No 
	FROM EDI_Outbound_Header  with (nolock)
	WHERE EDI_Outbound_Header.Project_Id = :as_project 
	AND EDI_Outbound_Header.EDI_Batch_Seq_No = Delivery_Master.EDI_Batch_Seq_No
	AND EDI_Outbound_Header.FTP_File_Name like '%\DM%') )
UNION ALL
SELECT  Receive_Master.WH_Code,   
         Receive_Master.Supp_Invoice_No,   
         'I' as Ord_Type,  
         Receive_Master.Ord_Date as Ord_Date,   
         Receive_Master.Ord_Status,   
         null as Delivery_Date, 
         Receive_Master.Complete_Date as Receive_Date 
FROM Receive_Master with (nolock)
INNER JOIN Customer with (nolock) 
ON Receive_Master.User_Field6 = Customer.Cust_Code
AND Customer.Project_Id = :as_project
AND Customer.User_Field1 = 'MIM'
WHERE ( Receive_Master.Project_Id = :as_project ) 
and ((Ord_Status <> 'C' and Receive_Master.Ord_Date>= getdate()-60) Or (Ord_Status = 'C' and Complete_Date > getdate()-10))
" retrieve.asneeded=yes arguments=(("as_project", string))  sort="ord_date D " )
