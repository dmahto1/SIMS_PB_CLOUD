table(column=(type=char(50) updatewhereclause=yes name=sku dbname="SKU" )
 column=(type=char(1) updatewhereclause=yes name=inventory_type dbname="inventory_type" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=c_avail_qty dbname="c_avail_Qty" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=c_alloc_qty dbname="c_alloc_qty" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=c_sit_qty dbname="c_sit_qty" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=c_wip_qty dbname="c_wip_qty" )
 retrieve="  SELECT Content_summary.SKU,   
         Content_summary.inventory_type, 
        	Sum( Content_Summary.Avail_Qty  ) as c_avail_Qty,
			Sum( Content_Summary.alloc_Qty  ) as c_alloc_qty, 
			Sum( Content_Summary.sit_Qty  ) as c_sit_qty, 
			Sum( Content_Summary.wip_Qty  ) as c_wip_qty 
    FROM Content_summary  With (NOLOCK)  
Where project_id = :asProject and 
		(Ro_no in (select Ro_No from Receive_Putaway where sku = sku_parent) or
		Ro_no in (select wo_No from WorkOrder_Putaway where sku = sku_parent)) and
		(avail_qty > 0 or alloc_qty > 0 or sit_qty > 0 or wip_qty > 0)

Group By Sku, Inventory_type





" arguments=(("asProject", string))  sort="sku A " )
