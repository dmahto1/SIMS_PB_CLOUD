table(column=(type=char(10) updatewhereclause=yes name=wh_code dbname="WH_Code" )
 column=(type=char(50) updatewhereclause=yes name=sku dbname="SKU" )
 column=(type=char(1) updatewhereclause=yes name=old_inventory_type dbname="Old_Inventory_Type" )
 column=(type=char(1) updatewhereclause=yes name=inventory_type dbname="Inventory_Type" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=old_quantity dbname="Old_quantity" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=quantity dbname="Quantity" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=net_adjust dbname="Net_adjust" )
 retrieve="  SELECT          
         Adjustment.WH_Code,  
	    Adjustment.SKU,   
         Adjustment.Old_Inventory_Type,   
         Adjustment.Inventory_Type,   
         sum(Adjustment.Old_quantity) as Old_quantity,   
         sum(Adjustment.Quantity) as Quantity,
 sum(Adjustment.Old_quantity)  -  sum(Adjustment.Quantity) as Net_adjust
    FROM Adjustment  With (NOLOCK)
   WHERE ( Adjustment.Project_ID = 'COMCAST' ) AND  
         ( Adjustment.Last_Update >= :adFromDate ) AND  
         ( Adjustment.Last_Update <= :adToDate )    AND
	    ( Adjustment.Old_Inventory_Type <>  Adjustment.Inventory_Type)
Group By 
          Adjustment.WH_Code,  
	    Adjustment.SKU,   
         Adjustment.Old_Inventory_Type,   
         Adjustment.Inventory_Type
" arguments=(("adFromDate", string),("adToDate", string))  sort="wh_code A sku A old_inventory_type A " )
