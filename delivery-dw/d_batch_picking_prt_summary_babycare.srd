 retrieve=" SELECT dbo.Delivery_Picking.SKU,   
		dbo.Delivery_Picking.L_Code		L_CODE, 
         Sum(dbo.Delivery_Picking.Quantity) as Quantity, 
		Item_master.Native_Description		Description,
		Delivery_Picking.Lot_no,
		Location.l_code		'forward_pick_loc'
    FROM dbo.Delivery_Master
    INNER JOIN dbo.Delivery_Picking
       ON Delivery_MAster.Do_no = Delivery_Picking.do_no
      AND Delivery_Picking.batch_pick_id = :batchID
   	INNER JOIN Item_Master   
	   ON Item_Master.Project_ID = Delivery_MAster.Project_ID 
	  and Item_MAster.SKU = Delivery_Picking.SKU 
	  and Item_Master.Supp_Code = Delivery_Picking.supp_Code
	LEFT OUTER JOIN Location
	  ON Delivery_Master.wh_code = Location.wh_code
	 AND Delivery_Picking.SKU = Location.SKU_Reserved
  Where 	Delivery_Master.Project_id = :asProject
Group by Delivery_Master.Project_id
,Delivery_Picking.Sku
,dbo.Delivery_Picking.L_Code
,Item_master.Native_Description
,Delivery_Picking.Lot_no     
,Location.l_code" arguments=(("asProject", string),("batchID", number))  sort="l_code A sku A " )
