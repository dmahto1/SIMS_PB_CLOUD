 retrieve="  SELECT DISTINCT ' ' as c_select_ind,   
         Delivery_Master.Scanner_ID,   
         Delivery_Master.Scanner_status,   
         Delivery_Master.WH_Code,   
         Delivery_Master.Ord_Type,   
         Delivery_Master.Invoice_No,   
         Delivery_Master.Cust_Order_No,   
         Delivery_Master.Ord_Date,   
         Delivery_Master.Schedule_Date,   
         Delivery_Master.DO_No,   
         Delivery_Master.Ord_Status,   
         Delivery_Master.Last_User,   
         Delivery_Master.Last_Update  
    FROM scanner_master,   
         Delivery_Master  
   WHERE ( scanner_master.Scanner_ID = Delivery_Master.Scanner_ID ) AND  
         ( Delivery_Master.Project_ID = :as_project_id  ) AND  
         ( Delivery_Master.Ord_Status = 'P'  ) AND  
         ( Delivery_Master.Scanner_status = 'READY'  )   
           
" update="Delivery_Master" updatewhere=1 updatekeyinplace=yes arguments=(("as_project_id", string)) )
