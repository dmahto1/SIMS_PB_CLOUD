 column=(type=char(20) updatewhereclause=yes name=supp_code_child dbname="Supp_Code_Child" )
 column=(type=decimal(5) updatewhereclause=yes name=item_component_child_qty dbname="Child_Qty" )
 column=(type=char(20) updatewhereclause=yes name=receive_detail_supp_code dbname="Supp_Code" )
 column=(type=char(70) updatewhereclause=yes name=item_master_description dbname="Description" )
 retrieve="  SELECT Receive_MAster.Project_ID, 
			Receive_Master.Supp_Invoice_No,   
			Receive_Master.wh_code, 
         Receive_Detail.SKU,   
         Receive_Detail.Alloc_Qty,   
         Item_Component.SKU_Child,   
         Item_Component.Supp_Code_Child,   
         Item_Component.Child_Qty,   
         Receive_Detail.Supp_Code,   
         Item_Master.Description  
    FROM Item_Component,   
         Receive_Detail,   
         Receive_Master,   
         Item_Master  
   WHERE ( Receive_Master.RO_No = Receive_Detail.RO_No ) and  
         ( Receive_Detail.SKU = Item_Component.SKU_Parent ) and  
         ( Receive_Detail.Supp_Code = Item_Component.Supp_Code_Parent ) and  
         ( Item_Master.Project_ID = Item_Component.Project_ID ) and  
         ( Item_Master.SKU = Item_Component.SKU_Child ) and  
         ( Item_Master.Supp_Code = Item_Component.Supp_Code_Child )  and
			Item_component.Component_type = 'P' and
			Receive_Detail.Alloc_qty > 0  
"  sort="project_id A sku_child A supp_code_child A receive_detail_sku A receive_master_supp_invoice_no A " )
